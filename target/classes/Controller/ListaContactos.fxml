<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="598.0" prefWidth="462.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ListaContactosController">
   <children>
      <HBox fx:id="hbCabezera" alignment="CENTER" prefHeight="100.0" prefWidth="462.0" spacing="50.0">
         <children>
            <Label fx:id="lbcontactos" prefHeight="70.0" prefWidth="365.0" text="Contactos">
               <font>
                  <Font size="48.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <TextField fx:id="txtBuscador" onMouseClicked="#limpiarTxtBuscador" prefHeight="26.0" prefWidth="269.0" text="Buscar contacto..." />
         </children>
         <padding>
            <Insets right="10.0" />
         </padding>
      </HBox>
      <VBox fx:id="vblista" layoutY="100.0" prefHeight="431.0" prefWidth="464.0" />
      <HBox fx:id="hbAnadir" alignment="CENTER_RIGHT" layoutX="-1.0" layoutY="548.0" prefHeight="70.0" prefWidth="464.0">
         <children>
            <Pane prefHeight="70.0" prefWidth="469.0">
               <children>
                  <Button fx:id="visualizarButton" alignment="BOTTOM_LEFT" layoutX="41.0" layoutY="22.0" mnemonicParsing="false" onAction="#visualizarInfo" text="Visualizar " />
                  <Button fx:id="btagregar" layoutX="324.0" layoutY="17.0" mnemonicParsing="false" onAction="#VentanaCrear" prefHeight="35.0" prefWidth="109.0" text="Anadir contacto" />
               </children>
            </Pane>
         </children>
      </HBox>
   </children>
</AnchorPane>
